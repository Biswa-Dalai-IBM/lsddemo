service fs1 (
    input {
        String count;
        String[] s1;
    }
    output {
        String out;
    }
) {
    MAP {
        mapTarget {
            String count;
            String[] s1;
            String temp;
        }
        set temp = "1";
    };
    WHILE(count>temp) {
        INVOKE pub.math:addInts {
            comment: ;
            label: null;
            timeout: ;
            input {
                mapSource {
                    String count;
                    String[] s1;
                    String temp;
                    String $iterationCount;
                }
                mapTarget[addIntsInput] {
                    String num1;
                    String num2;
                }
                copy temp -> num1;
                set num2 = "1";
            }
            output {
                mapSource[addIntsOutput] {
                    String value;
                }
                mapTarget {
                    String count;
                    String[] s1;
                    String temp;
                    String num1;
                    String num2;
                    String value;
                }
                copy value -> temp;
            }
        };
    };
    MAP {
        mapSource {
            String num1;
            String num2;
            String count;
            String[] s1;
            String temp;
            String value;
        }
        mapTarget {
            String count;
            String[] s1;
            String temp;
        }
        set temp = out;
        drop num1;
        drop num2;
        drop value;
    };
    LOOP {
        inputArray: "/s1";
        outputArray: "/s1";
        timeout: 11;
        SEQUENCE {
            exitOn: "FAILURE";
            MAP {
                label: "aa";
                mapSource {
                    String count;
                    String s1;
                    String temp;
                    String $iteration;
                }
                mapTarget {
                    String count;
                    String s1;
                    String temp;
                    String out;
                }
                copy s1 -> out;
            };
        };
    };
}
